class Solution {
    public String toLowerCase(String s) {
        String ans="";
        for(int i=0;i<s.length();i++){
            if(s.charAt(i)>='A'&&s.charAt(i)<='Z'){
                char temp=(char)(s.charAt(i)+32);
                ans+=temp;
            }
            else
                ans+=s.charAt(i);
        }
        return ans;
        
    }
}
